    $(document).ready(function () {
        // load markdown content at beginning
        var origMD = $('#Content').val();
        var theEditor = new tui.Editor({
            el: document.querySelector('#viewSection'),
            height: '300px',
            exts: ['youtube'],
        });

        theEditor.setMarkdown(origMD, false);
    });

</script>